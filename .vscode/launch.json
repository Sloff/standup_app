{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "env": {
        "STANDUP_ENV": "DEBUG",
        "STANDUP_CONFIG_DIR": "data",
        "STANDUP_REPORT_DIR": "data/reports"
      }
    },
    {
      "name": "Dart: Attach to Process",
      "type": "dart",
      "request": "attach",
      "program": "bin/standup_app.dart"
    },
    {
      "name": "Dart",
      "type": "dart",
      "request": "launch",
      "program": "bin/standup_app.dart",
      "args": ["add", "test", "-d", "2020-12-05"]
    },
    {
      "name": "standup_app",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "standup_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    }
  ]
}
